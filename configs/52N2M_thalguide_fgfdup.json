    // Global simulation parameters
    "steps" : 6000,                // Number of steps to simulate for
    "logevery": 1000,
    "overwrite_logs": true,
    "logbase" : "/home/seb/gdrive_usfd/data/BarrelEmerge/",
    "hextohex_d" : 0.0155,  // Hex to hex distance, determines num hexes. Odd bug shows if this is 0.015.
    "svgpath" : "./boundaries/whiskerbarrels_twofields3.svg",
    "boundaryFalloffDist" : 0.03,
    "D" : 0.2,
    "E" : 0.0,

    // gamma gain
    "G" : 1.0,

    // Exponent parameters
    "k" : 3,
    "l" : 1, // If not 1, then sim tends to blow up.

    // Timestep. Defaults to 0.00001 if omitted here
    "dt" : 0.0001,

    "contour_threshold" : 0.35,

