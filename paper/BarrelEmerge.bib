	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/cne.22531},
	keywords = {development, in situ hybridization, mouse, patterning, thalamus},
	pages = {528--543},
	file = {Full Text PDF:/home/seb/Zotero/storage/AY7UXFRC/Suzuki-Hirano et al. - 2011 - Dynamic spatiotemporal gene expression in embryoni.pdf:application/pdf;Snapshot:/home/seb/Zotero/storage/AN7NPSFF/cne.html:text/html}
}

