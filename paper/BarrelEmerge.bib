	pages = {755--769},
	file = {Greig et al. - 2013 - Molecular logic of neocortical projection neuron s.pdf:/home/seb/Zotero/storage/TKFPNXX6/Greig et al. - 2013 - Molecular logic of neocortical projection neuron s.pdf:application/pdf}
}

@article{fukuchi-shimogori_neocortex_2001,
	title = {Neocortex {Patterning} by the {Secreted} {Signaling} {Molecule} {FGF8}},
	volume = {294},
	issn = {0036-8075, 1095-9203},
	url = {http://science.sciencemag.org/content/294/5544/1071},
	doi = {10.1126/science.1064252},
