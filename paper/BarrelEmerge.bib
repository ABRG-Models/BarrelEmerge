	author = {Ant{\'o}n-Bola{\~n}os, Noelia and Sempere-Ferr{\`a}ndez, Alejandro and Guillam{\'o}n-Vivancos, Teresa and Martini, Francisco J. and P{\'e}rez-Saiz, Leticia and Gezelius, Henrik and Filipchuk, Anton and Valdeolmillos, Miguel and L{\'o}pez-Bendito, Guillermina},
	month = jun,
	year = {2019},
	pmid = {31048552},
	pages = {987--990},
	file = {Ant{\'o}n-Bola{\~n}os et al. - 2019 - Prenatal activity from thalamic neurons governs th.pdf:/home/seb/Zotero/storage/V4SXNSWX/Ant{\'o}n-Bola{\~n}os et al. - 2019 - Prenatal activity from thalamic neurons governs th.pdf:application/pdf;Ant{\'o}n-Bola{\~n}os et al. - 2019 - Prenatal activity from thalamic neurons governs th.pdf:/home/seb/Zotero/storage/WNXWJ75X/Ant{\'o}n-Bola{\~n}os et al. - 2019 - Prenatal activity from thalamic neurons governs th.pdf:application/pdf;Snapshot:/home/seb/Zotero/storage/7UBJP33Z/987.html:text/html}
}

