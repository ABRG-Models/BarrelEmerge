	month = apr,
	year = {2013},
	pmid = {23575831},
	pages = {6321--6332},
	annote = {Has good figure (Fig 1) of whole-brain expression of Lmo4 at several time points in development (E12.5 to P7)},
	file = {Full Text PDF:/home/seb/Zotero/storage/GNT6FWYQ/Cederquist et al. - 2013 - Lmo4 Establishes Rostral Motor Cortex Projection N.pdf:application/pdf;Snapshot:/home/seb/Zotero/storage/W3V635CZ/6321.html:text/html}
